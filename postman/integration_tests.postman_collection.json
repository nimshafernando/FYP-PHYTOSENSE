    {
      "name": "03 - QSAR Property Prediction",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test('QSAR prediction successful', function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "pm.test('QSAR response contains predictions', function () {",
              "    const response = pm.response.json();",
              "    pm.expect(response).to.have.property('prediction');",
              "    pm.expect(response).to.have.property('targets');",
              "    pm.expect(response).to.have.property('interpretations');",
              "});",
              "pm.test('Prediction values are numeric', function () {",
              "    const response = pm.response.json();",
              "    pm.expect(response.prediction).to.be.an('array');",
              "    pm.expect(response.prediction[0]).to.be.a('number');",
              "});"
            ],
            "type": "text/javascript"
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
