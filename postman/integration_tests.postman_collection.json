    {
      "name": "05 - AutoDock Vina Simulation",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test('Vina simulation completes successfully', function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "pm.test('Response contains docking results', function () {",
              "    const response = pm.response.json();",
              "    pm.expect(response).to.have.property('success');",
              "    pm.expect(response).to.have.property('poses');",
              "    pm.expect(response).to.have.property('compound_name');",
              "});",
              "pm.test('Binding poses are generated', function () {",
              "    const response = pm.response.json();",
              "    pm.expect(response.poses).to.be.an('array');",
              "    if (response.poses.length > 0) {",
              "        pm.expect(response.poses[0]).to.have.property('binding_affinity');",
              "    }",
              "});"
            ],
            "type": "text/javascript"
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
