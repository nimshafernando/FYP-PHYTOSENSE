    {
      "name": "04 - Calculate Molecular Descriptors",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test('Descriptors calculated successfully', function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "pm.test('Response contains molecular descriptors', function () {",
              "    const response = pm.response.json();",
              "    pm.expect(response).to.have.property('descriptors');",
              "    pm.expect(response.descriptors).to.have.property('MolWt');",
              "    pm.expect(response.descriptors).to.have.property('LogP');",
              "});",
              "pm.test('Molecular weight is reasonable', function () {",
              "    const response = pm.response.json();",
              "    pm.expect(response.descriptors.MolWt).to.be.above(100);",
              "    pm.expect(response.descriptors.MolWt).to.be.below(1000);",
              "});"
            ],
            "type": "text/javascript"
          }
        }
