      "name": "01 - Health Check",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test('Homepage loads successfully', function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "pm.test('Response contains HTML content', function () {",
              "    pm.expect(pm.response.text()).to.include('html');",
              "});"
            ],
            "type": "text/javascript"
          }
        }
      ],
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/",
          "host": ["{{base_url}}"],
          "path": [""]
        }
      }
    },
