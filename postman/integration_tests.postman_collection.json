      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test('Application is running and responsive', function () {",
              "    pm.expect(pm.response.code).to.be.oneOf([200, 404, 500]);",
              "});",
              "pm.test('Server responds within acceptable time', function () {",
              "    pm.expect(pm.response.responseTime).to.be.below(2000);",
              "});"
